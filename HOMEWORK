        <script>
            var rocket = ["Rockets ", "Space ", "Thrusters "];
            document.getElementById("q").innerHTML = rocket[0] + rocket[1] + rocket[2];

            document.getElementById("w").innerHTML = rocket.join(" * ")

            document.getElementById("e").innerHTML = rocket;
            rocket.pop();

            document.getElementById("r").innerHTML = rocket;

            var nikhil = {
                    name: "Nikhil",
                    age: "11",
                };

                
                document.getElementById("demo").innerHTML = "I am " + nikhil.name + ". I am " + nikhil.age + " years old. " + "I like " + rocket[0] + rocket[1];
        </script>
